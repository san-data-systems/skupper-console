{
  "name": "skupper-network-console",
  "version": "2.0.0",
  "author": "Skupper team",
  "homepage": "https://skupperproject.io",
  "description": "The Network Console is a user interface for monitoring and managing Skupper networks, providing observability, connectivity metrics, and integration with OpenShift.",
  "contributors": [
    {
      "name": "Skupper Team",
      "url": "https://skupperproject.io"
    }
  ],
  "license": "Apache-2.0",
  "keywords": [
    "skupper",
    "networking",
    "console",
    "monitoring",
    "observability",
    "connectivity",
    "openshift",
    "metrics",
    "kubernetes",
    "multicloud",
    "service-mesh"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skupperproject/skupper-console.git"
  },
  "bugs": {
    "url": "https://github.com/skupperproject/skupper-console/issues"
  },
  "private": true,
  "dependencies": {
    "@antv/g6": "5.0.42",
    "@nivo/sankey": "0.88.0",
    "@patternfly/patternfly": "6.1.0",
    "@patternfly/react-charts": "8.1.0",
    "@patternfly/react-core": "6.1.0",
    "@patternfly/react-icons": "6.1.0",
    "@patternfly/react-table": "6.1.0",
    "@patternfly/react-tokens": "6.1.0",
    "@tanstack/react-query": "5.63.0",
    "axios": "1.7.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "5.0.0",
    "react-router-dom": "7.1.1",
    "victory": "37.3.2"
  },
  "_comment": "Note: Victory 37.3.4 break legend in @patternfly/react-charts",
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.18.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "commitizen": "^4.3.1",
    "cz-git": "^1.11.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.3.0",
    "madge": "^8.0.0",
    "miragejs": "^0.1.48",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vite-plugin-favicons-inject": "^2.2.0",
    "vite-plugin-static-copy": "^2.2.0"
  },
  "optionalDependencies": {
    "cypress": "^13.17.0",
    "start-server-and-test": "^2.0.9"
  },
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "start": "vite --host",
    "preview": "vite preview",
    "test": "USE_MOCK_SERVER=true jest --config jest.config.ts",
    "coverage": "yarn test --coverage --collectCoverageFrom='src/**/*.{tsx,ts}'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "ci": "start-test 3000 cy:run",
    "lint": "TIMING=1 eslint --cache",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write '*/**/*.{ts,tsx,json,css}'",
    "check-cycles": "madge --circular --extensions ts,tsx src",
    "find-unused-modules": "ts-prune",
    "commit": "git-cz",
    "tsc:watch": "tsc --watch",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.19",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not op_mini all"
  ]
}
